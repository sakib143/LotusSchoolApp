<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <import type="android.view.View" />

        <variable
            name="viewmodel"
            type="com.appforschool.ui.commonwebview.CommonWebviewViewModel" />

        <variable
            name="listner"
            type="com.appforschool.ui.commonwebview.CommonWebviewActivity" />

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.commonwebview.CommonWebviewActivity">

        <WebView
            android:id="@+id/webView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            app:webviewUrl="@{viewmodel.webviewUrl}" />

    </RelativeLayout>

</layout>